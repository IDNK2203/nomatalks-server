<head>
  <meta charset="UTF-8" />

  <!-- page title -->
  <% if (typeof pageTitle !="undefined" ) { %>
  <title><%= `${pageTitle} | Nomatalks` %></title>

  <% }else{%>
  <title>Home | Nomatalks</title>
  <% } %>
  <!-- meta tags -->
  <% if(typeof blog !="undefined"){%>
  <meta name="description" content="<%=blog.snippet %>" />

  <% }else{%>
  <meta
    name="description"
    content="Welcome to nomatalks, We're dedicated to giving you the very best of entertaining contents, with a focus on being informative and inspiring."
  />
  <% }%>

  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta name="robots" content="index, follow" />
  <meta property="og:site_name" content="Nomatalks" />
  <!-- for blog posts use -->
  <% if(typeof pageTitle !="undefined"){%>
  <meta property="og:title" content="<%=`${pageTitle} | Nomatalks`%>" />
  <% }else{%>
  <meta property="og:title" content="Home | Nomatalks" />
  <% } %> <% if(typeof blog !="undefined"){%>

  <meta property="“og:type”" content="“article”" />
  <meta property="og:image" content="<%=blog.blogPostCI[0].url %>" />
  <meta property="twitter:image" content="<%=blog.blogPostCI[0].url %>" />
  <meta property="og:url" content="https://thenomatalks.com/<%= blog.slug %>" />
  <meta property="og:description" content="<%=blog.snippet %>" />

  <% }else{%>
  <meta
    property="og:image"
    content="https://thenomatalks.com/assets/nomatalks-logo.png"
  />
  <meta property="og:url" content="https://thenomatalks.com/" />
  <meta
    property="twitter:image"
    content="https://thenomatalks.com/assets/nomatalks-logo.png"
  />
  <meta property="og:type" content="profile" />
  <meta
    property="og:description"
    content="Welcome to nomatalks, your number one source for inspiring and engaging contents. We're dedicated to giving you the very best of entertaining contents, with a focus on being informative and inspiring."
  />
  <% }%>

  <meta property="twitter:card" content="summary_large_image" />
  <meta property="twitter:site" content="@nomatalks" />

  <!-- links -->
  <link
    rel="apple-touch-icon"
    sizes="180x180"
    href="/assets/apple-touch-icon.png"
  />
  <link
    rel="icon"
    type="image/png"
    sizes="32x32"
    href="/assets/favicon-32x32.png"
  />
  <link
    rel="icon"
    type="image/png"
    sizes="16x16"
    href="/assets/favicon-16x16.png"
  />
  <link rel="manifest" href="/assets/site.webmanifest" />
  <!-- <link
    rel="stylesheet"
    href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css"
    integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2"
    crossorigin="anonymous"
  /> -->
