<div class="form-group">
  <label for="issue">Issue</label>
  <input
    class="form-control"
    name="issue"
    id="issue"
    type="text"
    placeholder="Default input"
    value="<%= typeof magazine.issue != 'undefined' ? magazine.issue  : ''%>"
  />
</div>
<div class="form-group">
  <label for="snippet">Snippet</label>
  <input
    class="form-control"
    name="snippet"
    id="snippet"
    type="text"
    placeholder="Default input"
    value="<%= typeof magazine.snippet != 'undefined' ? magazine.snippet : ''%>"
  />
</div>
<div class="form-group">
  <label for="author">Author</label>
  <input
    class="form-control"
    name="author"
    id="author"
    type="text"
    placeholder="Default input"
    value="<%= typeof magazine.author != 'undefined' ? magazine.author : ''%>"
  />
</div>
<div class="form-group">
  <label for="digitalMagazine">Mazagine Pdf</label>
  <input
    type="file"
    class="form-control-file digitalMagazine"
    name="digitalMagazine"
    id="digitalMagazine"
  />
</div>
<div class="form-group">
  <label for="coverImage">Magazine cover Image </label>
  <input
    type="file"
    class="form-control-file coverImage"
    name="coverImage"
    id="coverImage"
  />
</div>

<button type="submit" class="btn btn-primary">Submit</button>
